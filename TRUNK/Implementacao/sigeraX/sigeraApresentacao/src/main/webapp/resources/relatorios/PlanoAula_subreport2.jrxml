	</parameter>
	<queryString>
		<![CDATA[SELECT
     plano.`id` AS plano_id,
     plano.`criterioavaliacao` AS plano_criterioavaliacao,
     plano.`datarealizacaoprovas` AS plano_datarealizacaoprovas,
     plano.`bibliografiasugerida` AS plano_bibliografiasugerida,
     plano.`turma_id` AS plano_turma_id,
     disciplina.`id` AS disciplina_id,
     disciplina.`bibliografia_basica` AS disciplina_bibliografia_basica,
     disciplina.`bibliografia_complementar` AS disciplina_bibliografia_complementar,
     turma.`id` AS turma_id,
     turma.`disciplina_id` AS turma_disciplina_id,
     req_plano.`Requerimento_id` AS req_plano_Requerimento_id,
     req_plano.`Plano_id` AS req_plano_Plano_id,
     requerimento.`id` AS requerimento_id,
     requerimento.`usuario_id` AS requerimento_usuario_id,
     requerimento.`data_abertura` AS requerimento_data_abertura,
     requerimento.`status_req` AS requerimento_status_req,
     requerimento.`data_fechamento` AS requerimento_data_fechamento,
     requerimento.`justificativa` AS requerimento_justificativa,
     requerimento.`tipo` AS requerimento_tipo
FROM
     `disciplina` disciplina INNER JOIN `turma` turma ON disciplina.`id` = turma.`disciplina_id`
     INNER JOIN `plano` plano ON turma.`id` = plano.`turma_id`
     INNER JOIN `req_plano` req_plano ON plano.`id` = req_plano.`Plano_id`
     INNER JOIN `requerimento` requerimento ON req_plano.`Requerimento_id` = requerimento.`id`
WHERE
     req_plano.`Plano_id` = $P{planoId}]]>
	</queryString>
